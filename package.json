{
    "name": "react-redux-boilerplate-isomorphic",
    "version": "0.0.0",
    "description": "react",
    "main": "index.js",
    "scripts": {
        "start": "npm run build-dev-server && npm-run-all --parallel build-dev-server--watch start-dev-server start-webpack-dev-server",
        "prod": "node build/server.js",
        "build": "npm run build-server; npm run build-client",
        "build-client": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --display-error-details --colors --optimize-minimize",
        "build-server": "cross-env NODE_ENV=production ./node_modules/.bin/webpack --display-error-details --colors --optimize-minimize --config webpack.server.config.js",
        "build-dev-server--watch": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --display-error-details --colors --config webpack.server.config.js --watch",
        "build-dev-server": "cross-env NODE_ENV=development ./node_modules/.bin/webpack --display-error-details --colors --config webpack.server.config.js",
        "start-dev-server": "cross-env NODE_ENV=development nodemon ./build/server.js --watch ./build",
        "start-webpack-dev-server": "cross-env NODE_ENV=development node ./server/webpackDevServer",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage"
    },
    "author": "alexolefirenko",
    "license": "ISC",
    "dependencies": {
        "cookie-parser": "^1.4.3",
        "css-loader": "^0.26.1",
        "data-fetcher": "^1.0.0-beta.5",
        "data-fetcher-redux": "^0.0.3",
        "express": "^4.15.4",
        "history": "^4.6.3",
        "http-proxy-middleware": "^0.17.4",
        "is-browser": "^2.0.1",
        "isomorphic-fetch": "^2.2.1",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-helmet": "^5.2.0",
        "react-hot-loader": "^3.1.3",
        "react-redux": "^5.0.6",
        "react-redux-loading-bar": "^2.6.5",
        "react-router": "^4.2.0",
        "react-router-config": "^1.0.0-beta.4",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.6",
        "react-router-resolver": "^1.0.0-beta.1",
        "redbox-react": "^1.3.3",
        "redux": "^3.7.2",
        "redux-thunk": "^2.2.0",
        "styled-components": "^2.0.0"
    },
    "devDependencies": {
        "autoprefixer": "^6.7.2",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^7.0.0",
        "babel-plugin-transform-decorators-legacy": "^1.3.4",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
        "babel-plugin-transform-runtime": "^6.22.0",
        "babel-polyfill": "^6.22.0",
        "babel-preset-env": "^1.6.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "babel-register": "^6.23.0",
        "cors": "^2.8.3",
        "cross-env": "^5.0.0",
        "enzyme": "^3.2.0",
        "eslint": "^4.8.0",
        "eslint-config-airbnb": "^16.0.0",
        "eslint-loader": "^1.6.1",
        "eslint-plugin-import": "^2.7.0",
        "eslint-plugin-jsx-a11y": "^6.0.2",
        "eslint-plugin-react": "^7.4.0",
        "extract-text-webpack-plugin": "^3.0.0",
        "file-loader": "^0.10.0",
        "jest": "^21.2.1",
        "jscs": "^3.0.7",
        "node-sass": "^4.5.0",
        "nodemon": "^1.11.0",
        "npm-run-all": "^4.0.2",
        "postcss-import": "^9.1.0",
        "progress-bar-webpack-plugin": "^1.10.0",
        "prop-types": "^15.5.8",
        "react-test-renderer": "^16.1.1",
        "sass-loader": "^6.0.6",
        "style-loader": "^0.13.1",
        "url-loader": "^0.5.7",
        "webpack": "^3.5.5",
        "webpack-dev-middleware": "^1.10.0",
        "webpack-hot-middleware": "^2.16.1"
    }
}
